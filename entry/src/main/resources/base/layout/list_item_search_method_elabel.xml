<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_content"
    ohos:width="match_parent"
    ohos:alignment="left|vertical_center"
    ohos:background_element="$graphic:bg_list_item_search_method"
    ohos:bottom_padding="10vp"
    ohos:margin="5vp"
    ohos:orientation="vertical"
    ohos:top_padding="10vp"

    >

    <DirectionalLayout
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:alignment="left|vertical_center"
        ohos:orientation="horizontal">

        <Image
            ohos:id="$+id:srced_image_png"
            ohos:height="45vp"
            ohos:width="80vp"
            ohos:end_margin="10vp"
            ohos:image_src="$media:test"
            ohos:scale_mode="stretch"
            ohos:start_margin="10vp"
            />

        <Text
            ohos:id="$+id:srced_name"
            ohos:height="match_content"
            ohos:width="match_parent"
            ohos:bottom_margin="10vp"
            ohos:end_margin="10vp"
            ohos:scrollable="true"
            ohos:start_margin="5vp"
            ohos:text="一二三四五六七八九十胶囊"
            ohos:text_alignment="left"
            ohos:text_color="$color:black_clear"
            ohos:text_size="24fp"
            ohos:text_weight="700"
            ohos:top_margin="10vp"
            />
    </DirectionalLayout>

    <Component
        ohos:height="1vp"
        ohos:width="match_parent"
        ohos:background_element="$color:black_line"
        ohos:end_margin="10vp"
        ohos:start_margin="10vp"
        ohos:top_margin="5vp"/>

    <DirectionalLayout
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:alignment="left|vertical_center"
        ohos:bottom_padding="5vp"
        ohos:end_margin="10vp"
        ohos:orientation="horizontal"
        ohos:start_margin="10vp"
        ohos:top_margin="10vp">

        <Text
            ohos:id="$+id:srced_elabel1"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:background_element="$graphic:bg_dtl_elabel_normal"
            ohos:bottom_margin="5vp"
            ohos:bottom_padding="3vp"
            ohos:end_margin="5vp"
            ohos:end_padding="10vp"
            ohos:start_margin="5vp"
            ohos:start_padding="10vp"
            ohos:text="测试标签"
            ohos:text_alignment="horizontal_center|vertical_center"
            ohos:text_color="$color:white"
            ohos:text_size="16fp"
            ohos:text_weight="500"
            ohos:top_margin="5vp"
            ohos:top_padding="3vp"
            ohos:visibility="hide"/>

        <Text
            ohos:id="$+id:srced_elabel2"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:background_element="$graphic:bg_dtl_elabel_normal"
            ohos:bottom_margin="5vp"
            ohos:bottom_padding="3vp"
            ohos:end_margin="5vp"
            ohos:end_padding="10vp"
            ohos:start_margin="5vp"
            ohos:start_padding="10vp"
            ohos:text="测试标签"
            ohos:text_alignment="horizontal_center|vertical_center"
            ohos:text_color="$color:white"
            ohos:text_size="16fp"
            ohos:text_weight="500"
            ohos:top_margin="5vp"
            ohos:top_padding="3vp"
            ohos:visibility="hide"/>

        <Text
            ohos:id="$+id:srced_elabel3"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:background_element="$graphic:bg_dtl_elabel_normal"
            ohos:bottom_margin="5vp"
            ohos:bottom_padding="3vp"
            ohos:end_margin="5vp"
            ohos:end_padding="10vp"
            ohos:start_margin="5vp"
            ohos:start_padding="10vp"
            ohos:text="测试标签"
            ohos:text_alignment="horizontal_center|vertical_center"
            ohos:text_color="$color:white"
            ohos:text_size="16fp"
            ohos:text_weight="500"
            ohos:top_margin="5vp"
            ohos:top_padding="3vp"
            ohos:visibility="hide"/>


    </DirectionalLayout>

    <DirectionalLayout
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:alignment="left|vertical_center"
        ohos:bottom_padding="10vp"
        ohos:end_margin="10vp"
        ohos:orientation="horizontal"
        ohos:start_margin="10vp"
        ohos:top_margin="5vp">

        <Text
            ohos:id="$+id:srced_elabel4"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:background_element="$graphic:bg_dtl_elabel_normal"
            ohos:bottom_margin="5vp"
            ohos:bottom_padding="3vp"
            ohos:end_margin="5vp"
            ohos:end_padding="10vp"
            ohos:start_margin="5vp"
            ohos:start_padding="10vp"
            ohos:text="测试标签"
            ohos:text_alignment="horizontal_center|vertical_center"
            ohos:text_color="$color:white"
            ohos:text_size="16fp"
            ohos:text_weight="500"
            ohos:top_margin="5vp"
            ohos:top_padding="3vp"
            ohos:visibility="hide"/>

        <Text
            ohos:id="$+id:srced_elabel5"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:background_element="$graphic:bg_dtl_elabel_normal"
            ohos:bottom_margin="5vp"
            ohos:bottom_padding="3vp"
            ohos:end_margin="5vp"
            ohos:end_padding="10vp"
            ohos:start_margin="5vp"
            ohos:start_padding="10vp"
            ohos:text="测试标签"
            ohos:text_alignment="horizontal_center|vertical_center"
            ohos:text_color="$color:white"
            ohos:text_size="16fp"
            ohos:text_weight="500"
            ohos:top_margin="5vp"
            ohos:top_padding="3vp"
            ohos:visibility="hide"/>


    </DirectionalLayout>

</DirectionalLayout>
