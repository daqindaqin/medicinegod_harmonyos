<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:width="match_parent"
    ohos:height="match_parent"
    ohos:alignment="top"
    ohos:background_element="$color:white">
        <ScrollView
            ohos:height="match_parent"
            ohos:width="match_parent"
            ohos:rebound_effect="true"
            ohos:bottom_margin="10vp">
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical"
                    ohos:alignment="horizontal_center|top">
                        <!--头部-->
                        <DirectionalLayout
                            ohos:id="$+id:home_top"
                            ohos:height="100vp"
                            ohos:width="match_parent"
                            ohos:alignment="horizontal_center|left"
                            ohos:orientation="horizontal">
                                <!--头像和等级-->
                                <DependentLayout
                                    ohos:height="match_parent"
                                    ohos:width="match_content">
                                        <Image
                                            ohos:id="$+id:home_image_head"
                                            ohos:height="60vp"
                                            ohos:width="60vp"
                                            ohos:bottom_margin="20vp"
                                            ohos:end_margin="20vp"
                                            ohos:image_src="$media:head"
                                            ohos:scale_mode="stretch"
                                            ohos:start_margin="25vp"
                                            ohos:top_margin="20vp"
                                            />
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text_alignment="horizontal_center"
                                            ohos:text="Lv999"
                                            ohos:text_size="12vp"
                                            ohos:text_weight="500"
                                            ohos:text_color="$color:white"
                                            ohos:background_element="$graphic:bg_home_level"
                                            ohos:top_margin="-28vp"
                                            ohos:start_margin="35vp"
                                            ohos:below="$id:home_image_head"
                                            ohos:padding="5vp"/>
                                </DependentLayout>
                                <!--名字和签名-->
                                <DirectionalLayout
                                    ohos:id="$+id:home_top_right"
                                    ohos:height="match_parent"
                                    ohos:width="match_parent"
                                    ohos:top_margin="20vp"
                                    ohos:end_margin="20vp"
                                    ohos:alignment="horizontal_center|left"
                                    ohos:orientation="vertical">

                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_parent"
                                            ohos:hint="这里是个用户名哈哈哈哈"
                                            ohos:layout_alignment="left|vertical_center"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="left"
                                            ohos:text_color="$color:black"
                                            ohos:text_size="26fp"
                                            ohos:text_weight="500"
                                            ohos:scrollable="true"/>

                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_parent"
                                            ohos:hint="这里是一条长长的签名用来好看的哈哈哈哈"
                                            ohos:layout_alignment="left|vertical_center"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="left"
                                            ohos:text_color="$color:black_clear"
                                            ohos:text_size="18fp"
                                            ohos:text_weight="500"
                                            ohos:scrollable="true"/>
                                </DirectionalLayout>
                        </DirectionalLayout>
                        <!--分割线-->
                        <Image
                            ohos:height="2vp"
                            ohos:width="match_parent"
                            ohos:background_element="$color:black_line"
                            ohos:top_margin="10vp"
                            ohos:bottom_margin="10vp"
                            ohos:below="$id:home_top"/>
                        <!--临期、过期、正常三个模块-->
                        <DirectionalLayout
                            ohos:height="match_content"
                            ohos:width="match_parent"
                            ohos:orientation="horizontal">
                                <DirectionalLayout
                                    ohos:height="100vp"
                                    ohos:width="100vp"
                                    ohos:margin="10vp"
                                    ohos:padding="10vp"
                                    ohos:orientation="vertical"
                                    ohos:alignment="horizontal_center|vertical_center"
                                    ohos:background_element="$graphic:bg_chat_moudle_background">
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="2"
                                            ohos:text_weight="700"
                                            ohos:text_color="$color:orange"
                                            ohos:text_size="26vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="临期"
                                            ohos:text_weight="500"
                                            ohos:text_color="$color:black_clear"
                                            ohos:text_size="14vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>

                                </DirectionalLayout>
                                <DirectionalLayout
                                    ohos:height="100vp"
                                    ohos:width="100vp"
                                    ohos:margin="10vp"
                                    ohos:padding="10vp"
                                    ohos:orientation="vertical"
                                    ohos:alignment="horizontal_center|vertical_center"
                                    ohos:background_element="$graphic:bg_chat_moudle_background">
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="17"
                                            ohos:text_weight="700"
                                            ohos:text_color="$color:red"
                                            ohos:text_size="26vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="过期"
                                            ohos:text_weight="500"
                                            ohos:text_color="$color:black_clear"
                                            ohos:text_size="14vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                </DirectionalLayout>
                                <DirectionalLayout
                                    ohos:height="100vp"
                                    ohos:width="100vp"
                                    ohos:margin="10vp"
                                    ohos:padding="10vp"
                                    ohos:orientation="vertical"
                                    ohos:alignment="horizontal_center|vertical_center"
                                    ohos:background_element="$graphic:bg_chat_moudle_background">
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="0"
                                            ohos:text_weight="700"
                                            ohos:text_color="$color:green"
                                            ohos:text_size="26vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="正常"
                                            ohos:text_weight="500"
                                            ohos:text_color="$color:black_clear"
                                            ohos:text_size="14vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                </DirectionalLayout>
                        </DirectionalLayout>
                        <!--持有、收藏、分享三个模块-->
                        <DirectionalLayout
                            ohos:height="match_content"
                            ohos:width="match_parent"
                            ohos:orientation="horizontal">
                                <DirectionalLayout
                                    ohos:height="100vp"
                                    ohos:width="100vp"
                                    ohos:margin="10vp"
                                    ohos:padding="10vp"
                                    ohos:orientation="vertical"
                                    ohos:alignment="horizontal_center|vertical_center"
                                    ohos:background_element="$graphic:bg_chat_moudle_background">
                                        <Text
                                            ohos:id="$+id:me_has"
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="99+"
                                            ohos:text_weight="700"
                                            ohos:text_color="$color:blue"
                                            ohos:text_size="26vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="持有"
                                            ohos:text_weight="500"
                                            ohos:text_color="$color:black_clear"
                                            ohos:text_size="14vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>

                                </DirectionalLayout>
                                <DirectionalLayout
                                    ohos:height="100vp"
                                    ohos:width="100vp"
                                    ohos:margin="10vp"
                                    ohos:padding="10vp"
                                    ohos:orientation="vertical"
                                    ohos:alignment="horizontal_center|vertical_center"
                                    ohos:background_element="$graphic:bg_chat_moudle_background">
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="17"
                                            ohos:text_weight="700"
                                            ohos:text_color="$color:blue"
                                            ohos:text_size="26vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                        <Text
                                            ohos:id="$+id:me_fav"
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="收藏"
                                            ohos:text_weight="500"
                                            ohos:text_color="$color:black_clear"
                                            ohos:text_size="14vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                </DirectionalLayout>
                                <DirectionalLayout
                                    ohos:height="100vp"
                                    ohos:width="100vp"
                                    ohos:margin="10vp"
                                    ohos:padding="10vp"
                                    ohos:orientation="vertical"
                                    ohos:alignment="horizontal_center|vertical_center"
                                    ohos:background_element="$graphic:bg_chat_moudle_background">
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="0"
                                            ohos:text_weight="700"
                                            ohos:text_color="$color:blue"
                                            ohos:text_size="26vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                        <Text
                                            ohos:id="$+id:me_share"
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="分享"
                                            ohos:text_weight="500"
                                            ohos:text_color="$color:black_clear"
                                            ohos:text_size="14vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                </DirectionalLayout>
                        </DirectionalLayout>
                        <!--动态、关注、粉丝三个模块-->
                        <DirectionalLayout
                            ohos:height="match_content"
                            ohos:width="match_parent"
                            ohos:orientation="horizontal"
                            ohos:visibility="hide">
                                <DirectionalLayout
                                    ohos:height="100vp"
                                    ohos:width="100vp"
                                    ohos:margin="10vp"
                                    ohos:padding="10vp"
                                    ohos:orientation="vertical"
                                    ohos:alignment="horizontal_center|vertical_center"
                                    ohos:background_element="$graphic:bg_chat_moudle_background">
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="3"
                                            ohos:text_weight="700"
                                            ohos:text_color="$color:blue"
                                            ohos:text_size="26vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="动态"
                                            ohos:text_weight="500"
                                            ohos:text_color="$color:black_clear"
                                            ohos:text_size="14vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>

                                </DirectionalLayout>
                                <DirectionalLayout
                                    ohos:height="100vp"
                                    ohos:width="100vp"
                                    ohos:margin="10vp"
                                    ohos:padding="10vp"
                                    ohos:orientation="vertical"
                                    ohos:alignment="horizontal_center|vertical_center"
                                    ohos:background_element="$graphic:bg_chat_moudle_background">
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="3636"
                                            ohos:text_weight="700"
                                            ohos:text_color="$color:blue"
                                            ohos:text_size="26vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="关注"
                                            ohos:text_weight="500"
                                            ohos:text_color="$color:black_clear"
                                            ohos:text_size="14vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                </DirectionalLayout>
                                <DirectionalLayout
                                    ohos:height="100vp"
                                    ohos:width="100vp"
                                    ohos:margin="10vp"
                                    ohos:padding="10vp"
                                    ohos:orientation="vertical"
                                    ohos:alignment="horizontal_center|vertical_center"
                                    ohos:background_element="$graphic:bg_chat_moudle_background">
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="110vp"
                                            ohos:text="896W"
                                            ohos:text_weight="700"
                                            ohos:text_color="$color:blue"
                                            ohos:text_size="26vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                        <Text
                                            ohos:height="match_content"
                                            ohos:width="match_content"
                                            ohos:text="粉丝"
                                            ohos:text_weight="500"
                                            ohos:text_color="$color:black_clear"
                                            ohos:text_size="14vp"
                                            ohos:margin="5vp"
                                            ohos:text_alignment="horizontal_center"/>
                                </DirectionalLayout>
                        </DirectionalLayout>

                        <Text
                            ohos:height="match_content"
                            ohos:width="match_parent"
                            ohos:text_size="18vp"
                            ohos:text="推荐好友"
                            ohos:text_color="$color:black_clear"
                            ohos:text_weight="500"
                            ohos:start_margin="10vp"
                            ohos:end_margin="10vp"
                            ohos:top_margin="10vp"
                            ohos:bottom_margin="5vp"
                            ohos:padding="10vp"
                            ohos:background_element="$graphic:bg_chat_moudle_background"
                            />
                        <Text
                            ohos:height="match_content"
                            ohos:width="match_parent"
                            ohos:text_size="18vp"
                            ohos:text="常见问题"
                            ohos:text_color="$color:black_clear"
                            ohos:text_weight="500"
                            ohos:start_margin="10vp"
                            ohos:end_margin="10vp"
                            ohos:top_margin="5vp"
                            ohos:bottom_margin="5vp"
                            ohos:padding="10vp"
                            ohos:background_element="$graphic:bg_chat_moudle_background"
                            />
                        <Text
                            ohos:height="match_content"
                            ohos:width="match_parent"
                            ohos:text_size="18vp"
                            ohos:text="反馈建议"
                            ohos:text_color="$color:black_clear"
                            ohos:text_weight="500"
                            ohos:start_margin="10vp"
                            ohos:end_margin="10vp"
                            ohos:top_margin="5vp"
                            ohos:bottom_margin="5vp"
                            ohos:padding="10vp"
                            ohos:background_element="$graphic:bg_chat_moudle_background"
                            />
                        <Text
                            ohos:height="match_content"
                            ohos:width="match_parent"
                            ohos:text_size="18vp"
                            ohos:text="关于"
                            ohos:text_color="$color:black_clear"
                            ohos:text_weight="500"
                            ohos:start_margin="10vp"
                            ohos:end_margin="10vp"
                            ohos:top_margin="5vp"
                            ohos:bottom_margin="5vp"
                            ohos:padding="10vp"
                            ohos:background_element="$graphic:bg_chat_moudle_background"
                            />
                        <Text
                            ohos:height="match_content"
                            ohos:width="match_parent"
                            ohos:text_size="18vp"
                            ohos:text="设置"
                            ohos:text_color="$color:black_clear"
                            ohos:text_weight="500"
                            ohos:start_margin="10vp"
                            ohos:end_margin="10vp"
                            ohos:top_margin="5vp"
                            ohos:bottom_margin="5vp"
                            ohos:padding="10vp"
                            ohos:background_element="$graphic:bg_chat_moudle_background"
                            />



                </DirectionalLayout>
        </ScrollView>





</DependentLayout>