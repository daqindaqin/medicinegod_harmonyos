<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:width="match_parent"
    ohos:height="match_parent"
    ohos:alignment="top"
    ohos:background_element="$color:white">
        <ScrollView
            ohos:height="match_parent"
            ohos:width="match_parent"
            ohos:bottom_margin="10vp">
                <DirectionalLayout
                    ohos:height="match_content"
                    ohos:width="match_parent"
                    ohos:orientation="vertical"
                    ohos:alignment="horizontal_center|top">
                        <Image
                            ohos:id="$+id:me_dtl_head"
                            ohos:height="80vp"
                            ohos:width="80vp"
                            ohos:bottom_margin="20vp"
                            ohos:end_margin="20vp"
                            ohos:image_src="$media:head"
                            ohos:scale_mode="stretch"
                            ohos:start_margin="25vp"
                            ohos:top_margin="30vp"
                            />
                        <DirectionalLayout
                            ohos:height="match_content"
                            ohos:width="match_parent"
                            ohos:start_margin="25vp"
                            ohos:end_margin="25vp"
                            ohos:orientation="vertical"
                            ohos:alignment="vertical_center|left"
                            ohos:background_element="$graphic:bg_chat_moudle_background">
                                <Text
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="会员到期时间:"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="10vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    />
                                <Text
                                    ohos:id="$+id:me_dtl_vipdate"
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="VIP已到期！"
                                    ohos:hint_color="$color:black_line"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="5vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    ohos:background_element="$graphic:bg_chat_moudle_background"
                                    />
                                <Text
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="登录名:"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="10vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    />
                                <Text
                                    ohos:id="$+id:me_dtl_lname"
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="默认登录名"
                                    ohos:hint_color="$color:black_line"
                                    ohos:text_color="$color:black_clear"
                                    ohos:max_text_lines="1"
                                    ohos:multiple_lines="false"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="5vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    ohos:background_element="$graphic:bg_chat_moudle_background"
                                    />
                                <Text
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="外显名:"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="10vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    />
                                <Text
                                    ohos:id="$+id:me_dtl_sname"
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="默认外显示名"
                                    ohos:hint_color="$color:black_line"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="5vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    ohos:background_element="$graphic:bg_chat_moudle_background"
                                    />
                                <Text
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="密码:"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="10vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    />
                                <Text
                                    ohos:id="$+id:me_dtl_pwd"
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="*********"
                                    ohos:hint_color="$color:black_line"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="5vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    ohos:background_element="$graphic:bg_chat_moudle_background"
                                    />
                                <Text
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="绑定手机:"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="10vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    />
                                <Text
                                    ohos:id="$+id:me_dtl_phone"
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="默认绑定手机"
                                    ohos:hint_color="$color:black_line"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="5vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    ohos:background_element="$graphic:bg_chat_moudle_background"
                                    />
                                <Text
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="绑定邮箱:"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="10vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    />
                                <Text
                                    ohos:id="$+id:me_dtl_mail"
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="默认绑定邮箱"
                                    ohos:hint_color="$color:black_line"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="5vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    ohos:background_element="$graphic:bg_chat_moudle_background"
                                    />
                                <Text
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="注册时间:"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="10vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    />
                                <Text
                                    ohos:id="$+id:me_dtl_rgtime"
                                    ohos:height="match_content"
                                    ohos:width="match_parent"
                                    ohos:text_size="18vp"
                                    ohos:text="1970-1-1"
                                    ohos:hint_color="$color:black_line"
                                    ohos:text_color="$color:black_clear"
                                    ohos:text_weight="500"
                                    ohos:start_margin="10vp"
                                    ohos:end_margin="10vp"
                                    ohos:top_margin="5vp"
                                    ohos:bottom_margin="5vp"
                                    ohos:padding="10vp"
                                    ohos:background_element="$graphic:bg_chat_moudle_background"
                                    />


                        </DirectionalLayout>






                </DirectionalLayout>
        </ScrollView>





</DependentLayout>