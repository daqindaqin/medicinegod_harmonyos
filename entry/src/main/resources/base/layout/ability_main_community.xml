<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:alignment="top"
    ohos:background_element="$color:white"
    >

    <Text
        ohos:id="$+id:community_title"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:bottom_padding="20vp"
        ohos:text="世  界"
        ohos:text_alignment="vertical_center|horizontal_center"
        ohos:text_color="$color:black_clear"
        ohos:text_size="24fp"
        ohos:text_weight="700"
        ohos:top_padding="20vp"/>

    <Image
        ohos:id="$+id:community_line0"
        ohos:height="3"
        ohos:width="match_parent"
        ohos:background_element="$color:black_line"
        ohos:below="$id:community_title"
        ohos:bottom_margin="5vp"/>


    <DirectionalLayout
        ohos:height="match_parent"
        ohos:width="match_parent"
        ohos:below="$id:community_line0"
        ohos:orientation="vertical">

        <ListContainer
            ohos:id="$+id:community_contextlist"
            ohos:height="match_parent"
            ohos:width="match_parent"
            ohos:layout_alignment="horizontal_center"
            ohos:margin="10vp"
            ohos:rebound_effect="true"
            />


    </DirectionalLayout>


</DependentLayout>