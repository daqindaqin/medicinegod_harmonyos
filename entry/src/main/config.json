{
  "app": {
    "bundleName": "com.daqin.medicinegod",
    "vendor": "daqin",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.daqin.medicinegod",
    "name": ".MyApplication",
    "mainAbility": "com.daqin.medicinegod.MainAbility",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "visible": true,
        "name": "com.daqin.medicinegod.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.Light.NoTitleBar"
        }
      ]
    },
    "reqPermissions": [
      {
        "name": "ohos.permission.READ_MEDIA",
        "reason": "选择上传图片使用",
        "usedScene":
        {
          "ability": ["com.daqin.medicinegod.MainAbility"],
          "when": "always"
        }
      }
    ]

  }
}